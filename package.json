{
	"name": "wordpressify",
	"version": "0.3.0",
	"description": "Automate your WordPress development workflow.",
	"keywords": [
		"babel",
		"boilerplate",
		"build",
		"css",
		"es6",
		"front-end",
		"gulp",
		"javascript",
		"node",
		"postcss",
		"sass",
		"web",
		"website",
		"wordpress",
		"wordpressify",
		"workflow",
		"docker",
		"cherry"
	],
	"homepage": "https://www.wordpressify.co",
	"repository": "https://github.com/luangjokaj/wordpressify",
	"main": "./installer/index.js",
	"bin": {
		"wordpressify": "./installer/index.js"
	},
	"scripts": {
		"format": "wp-scripts format-js"
	},
	"author": "Luan Gjokaj <luan@riangle.com>",
	"license": "MIT",
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,ts,tsx}": [
			"wp-scripts format-js"
		]
	},
	"dependencies": {
		"chalk": "^4.1.0",
		"commander": "^7.2.0",
		"download": "^8.0.0",
		"execa": "^5.0.0",
		"ora": "^5.4.0",
		"prompts": "^2.4.1"
	},
	"devDependencies": {
		"@wordpress/scripts": "^14.0.1",
		"husky": "^6.0.0",
		"lint-staged": "^10.5.4"
	}
}
